<%= form_for @specialty do |f| %>
	<% if @specialty.errors.any? %>
		<div id="error_explanation">
			<h2><%= pluralize(@specialty.errors.count, "error") %> prohibido guardar specialty:</h2>

		<ul>
		<% @specialty.errors.full_messages.each do |msg| %>
			<li><%= msg %></li>
		<% end %>
		</ul>
		</div>
	<% end %>
	<p>
		<%= f.label :company_id %><br />
		<%= f.text_field :company_id %>
	</p>
	<p>
		<%= f.label :name %><br />
		<%= f.text_field :name %>
	</p>
	<p>
		<%= f.label :code %><br />
		<%= f.text_field :code %>
	</p>
	<p>
		<%= f.label :default %><br />
		<%= f.check_box :default %>
	</p>
	<p>
		<%= f.label :suspension_type_id %><br />
		<%= f.text_field :suspension_type_id %>
	</p>
	<p>
		<%= f.label :notes %><br />
		<%= f.text_field :notes %>
	</p>
	<p><%= f.submit %></p>
<% end %>
