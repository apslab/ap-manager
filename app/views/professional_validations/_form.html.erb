<%= form_for @professional_validation do |f| %>
	<% if @professional_validation.errors.any? %>
		<div id="error_explanation">
			<h2><%= pluralize(@professional_validation.errors.count, "error") %> prohibido guardar Professional Validation:</h2>
			<ul>
			<% @professional_validation.errors.full_messages.each do |msg| %>
				<li><%= msg %></li>
			<% end %>
			</ul>
		</div>
	<% end %>
	<table class="data" style="width:100%" >
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :professional_id %></strong></td>
			<td style="width:70%">
				<%= f.text_field :professional_id %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :valid_since %></strong></td>
			<td style="width:70%">
				<%= f.text_field :valid_since, {:class => "dtPicker"} %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :valid_until %></strong></td>
			<td style="width:70%">
				<%= f.text_field :valid_until, {:class => "dtPicker"} %>
			</td>
		</tr>
	</table>
	<p><%= f.submit %></p>
<% end %>
