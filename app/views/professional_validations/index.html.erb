<h1>Professional Validations</h1>

<table>
	<tr>
		<th>Professional</th>
		<th>Valid Since</th>
		<th>Valid Until</th>
		<th></th>
		<th></th>
		<th></th>
	</tr>
	<% for professional_validation in @professional_validations %>
		<tr>
			<td><%= professional_validation.professional_id %></td>
			<td><%= professional_validation.valid_since %></td>
			<td><%= professional_validation.valid_until %></td>
            <td><a href="/professional_validations/<%= professional_validation.id.to_s %>"><img alt="Ver" src="images/icons/16x16/application_get.png" title="Ver" /></a></td>
            <td><a href="/professional_validations/<%= professional_validation.id.to_s %>/edit"><img alt="Editar" src="images/icons/16x16/application_edit.png" title="Editar" /></a></td>
            <td><a href="/professional_validations/<%= professional_validation.id.to_s %>" data-confirm="Esta ud. seguro?" data-method="delete" rel="nofollow">
				<img alt="Eliminar" src="images/icons/16x16/application_delete.png" title="Eliminar" /></a>
			</td>
		</tr>
	<% end %>
</table>
<p><%= link_to "Nuevo Professional Validation", new_professional_validation_path %></p>
