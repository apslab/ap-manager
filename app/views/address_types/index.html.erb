<h1>Address Types</h1>

<table>
	<tr>
		<th>Company</th>
		<th>Name</th>
		<th>Code</th>
		<th>Default</th>
		<th></th>
		<th></th>
		<th></th>
	</tr>
	<% for address_type in @address_types %>
		<tr>
			<td><%= address_type.company_id %></td>
			<td><%= address_type.name %></td>
			<td><%= address_type.code %></td>
			<td><%= address_type.default %></td>
			<td><%= link_to "Ver", address_type %></td>
			<td><%= link_to "Editar", edit_address_type_path(address_type) %></td>
			<td><%= link_to "Eliminar", address_type, :confirm => 'Esta seguro?', :method => :delete %></td>
		</tr>
	<% end %>
</table>
<p><%= link_to "Nuevo Address Type", new_address_type_path %></p>
