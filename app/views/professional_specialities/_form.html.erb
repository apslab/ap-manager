<%= form_for @professional_speciality do |f| %>
	<% if @professional_speciality.errors.any? %>
		<div id="error_explanation">
			<h2><%= pluralize(@professional_speciality.errors.count, "error") %> prohibido guardar Professional Speciality:</h2>
			<ul>
			<% @professional_speciality.errors.full_messages.each do |msg| %>
				<li><%= msg %></li>
			<% end %>
			</ul>
		</div>
	<% end %>
	<table class="data" style="width:100%" >
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :professional_id %></strong></td>
			<td style="width:70%">
				<%= f.text_field :professional_id %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :specialty_id %></strong></td>
			<td style="width:70%">
				<%= f.collection_select :specialty_id, Specialty.all, :id, :name %>
			</td>
		</tr>
	</table>
	<p><%= f.submit %></p>
<% end %>
