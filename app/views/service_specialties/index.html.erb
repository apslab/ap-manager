<h1>Especialidades por Servicio</h1>

<table>
	<tr>
		<th>Service</th>
		<th>Name</th>
		<th>Specialty</th>
		<th></th>
		<th></th>
		<th></th>
	</tr>
	<% for service_specialty in @service_specialties %>
		<tr>
			<td><%= service_specialty.service_id %></td>
			<td><%= service_specialty.name %></td>
			<td><%= service_specialty.specialty_id %></td>
            <td><a href="/service_specialties/<%= service_specialty.id.to_s %>"><img alt="Ver" src="images/icons/16x16/application_get.png" title="Ver" /></a></td>
            <td><a href="/service_specialties/<%= service_specialty.id.to_s %>/edit"><img alt="Editar" src="images/icons/16x16/application_edit.png" title="Editar" /></a></td>
            <td><a href="/service_specialties/<%= service_specialty.id.to_s %>" data-confirm="Esta ud. seguro?" data-method="delete" rel="nofollow">
				<img alt="Eliminar" src="images/icons/16x16/application_delete.png" title="Eliminar" /></a>
			</td>
		</tr>
	<% end %>
</table>
<p><%= link_to "Nuevo Especialidad por Servicio", new_service_specialty_path %></p>
