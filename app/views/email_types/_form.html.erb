<%= form_for @email_type do |f| %>
	<% if @email_type.errors.any? %>
		<div id="error_explanation">
			<h2><%= pluralize(@email_type.errors.count, "error") %> prohibido guardar email_type:</h2>

		<ul>
		<% @email_type.errors.full_messages.each do |msg| %>
			<li><%= msg %></li>
		<% end %>
		</ul>
		</div>
	<% end %>
	<p>
		<%= f.label :company_id %><br />
		<%= f.text_field :company_id %>
	</p>
	<p>
		<%= f.label :name %><br />
		<%= f.text_field :name %>
	</p>
	<p>
		<%= f.label :code %><br />
		<%= f.text_field :code %>
	</p>
	<p>
		<%= f.label :default %><br />
		<%= f.check_box :default %>
	</p>
	<p><%= f.submit %></p>
<% end %>
