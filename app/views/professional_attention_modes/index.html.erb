<h1>Professional Attention Modes</h1>

<table>
	<tr>
		<th>Professional</th>
		<th>Attention Mode</th>
		<th></th>
		<th></th>
		<th></th>
	</tr>
	<% for professional_attention_mode in @professional_attention_modes %>
		<tr>
			<td><%= professional_attention_mode.professional_id %></td>
			<td><%= professional_attention_mode.attention_mode_id %></td>
            <td><a href="/professional_attention_modes/<%= professional_attention_mode.id.to_s %>"><img alt="Ver" src="images/icons/16x16/application_get.png" title="Ver" /></a></td>
            <td><a href="/professional_attention_modes/<%= professional_attention_mode.id.to_s %>/edit"><img alt="Editar" src="images/icons/16x16/application_edit.png" title="Editar" /></a></td>
            <td><a href="/professional_attention_modes/<%= professional_attention_mode.id.to_s %>" data-confirm="Esta ud. seguro?" data-method="delete" rel="nofollow">
				<img alt="Eliminar" src="images/icons/16x16/application_delete.png" title="Eliminar" /></a>
			</td>
		</tr>
	<% end %>
</table>
<p><%= link_to "Nuevo Professional Attention Mode", new_professional_attention_mode_path %></p>
