<h1>Profesionales</h1>

<table>
	<tr>
		<th>Person</th>
		<th>Code</th>
		<th>National Enrollment</th>
		<th>State Enrollment</th>
		<th>Relationship Work</th>
		<th>Schedule Appointments</th>
		<th>On Demand Appointments</th>
		<th>Own</th>
		<th></th>
		<th></th>
		<th></th>
	</tr>
	<% for professional in @professionals %>
		<tr>
			<td><%= professional.person_id %></td>
			<td><%= professional.code %></td>
			<td><%= professional.national_enrollment %></td>
			<td><%= professional.state_enrollment %></td>
			<td><%= check_box_tag "eula", professional.relationship_work, professional.relationship_work, :disabled => true %></td>
			<td><%= check_box_tag "eula", professional.schedule_appointments, professional.schedule_appointments, :disabled => true %></td>
			<td><%= check_box_tag "eula", professional.on_demand_appointments, professional.on_demand_appointments, :disabled => true %></td>
			<td><%= check_box_tag "eula", professional.own, professional.own, :disabled => true %></td>
            <td><a href="/professionals/<%= professional.id.to_s %>"><img alt="Ver" src="images/icons/16x16/application_get.png" title="Ver" /></a></td>
            <td><a href="/professionals/<%= professional.id.to_s %>/edit"><img alt="Editar" src="images/icons/16x16/application_edit.png" title="Editar" /></a></td>
            <td><a href="/professionals/<%= professional.id.to_s %>" data-confirm="Esta ud. seguro?" data-method="delete" rel="nofollow">
				<img alt="Eliminar" src="images/icons/16x16/application_delete.png" title="Eliminar" /></a>
			</td>
		</tr>
	<% end %>
</table>
<p><%= link_to "Nuevo Profesional", new_professional_path %></p>
