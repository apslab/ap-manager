<h1>Especialidades por Nomenclador</h1>

<table>
	<tr>
		<th>Specialty</th>
		<th>Name</th>
		<th>Service</th>
		<th>Nomenclature</th>
		<th>Ambulatory Or Internship</th>
		<th></th>
		<th></th>
		<th></th>
	</tr>
	<% for specialty_nomenclature in @specialty_nomenclatures %>
		<tr>
			<td><%= specialty_nomenclature.specialty_id %></td>
			<td><%= specialty_nomenclature.name %></td>
			<td><%= specialty_nomenclature.service_id %></td>
			<td><%= specialty_nomenclature.nomenclature_id %></td>
			<td><%= specialty_nomenclature.ambulatory_or_internship %></td>
            <td><a href="/specialty_nomenclatures/<%= specialty_nomenclature.id.to_s %>"><img alt="Ver" src="images/icons/16x16/application_get.png" title="Ver" /></a></td>
            <td><a href="/specialty_nomenclatures/<%= specialty_nomenclature.id.to_s %>/edit"><img alt="Editar" src="images/icons/16x16/application_edit.png" title="Editar" /></a></td>
            <td><a href="/specialty_nomenclatures/<%= specialty_nomenclature.id.to_s %>" data-confirm="Esta ud. seguro?" data-method="delete" rel="nofollow">
				<img alt="Eliminar" src="images/icons/16x16/application_delete.png" title="Eliminar" /></a>
			</td>
		</tr>
	<% end %>
</table>
<p><%= link_to "Nueva Especialidad por Nomenclador", new_specialty_nomenclature_path %></p>
