<%= form_for @professional_scheduler do |f| %>
	<% if @professional_scheduler.errors.any? %>
		<div id="error_explanation">
			<h2><%= pluralize(@professional_scheduler.errors.count, "error") %> prohibido guardar Agenda del Profesional:</h2>
			<ul>
			<% @professional_scheduler.errors.full_messages.each do |msg| %>
				<li><%= msg %></li>
			<% end %>
			</ul>
		</div>
	<% end %>
	<table class="data" style="width:100%" >
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :professional_id %></strong></td>
			<td style="width:70%">
				<%= f.text_field :professional_id %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :specialty_id %></strong></td>
			<td style="width:70%">
				<%= f.collection_select :specialty_id, Specialty.all, :id, :name %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :d1 %></strong></td>
			<td style="width:70%">
				<%= f.check_box :d1 %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :d2 %></strong></td>
			<td style="width:70%">
				<%= f.check_box :d2 %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :d3 %></strong></td>
			<td style="width:70%">
				<%= f.check_box :d3 %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :d4 %></strong></td>
			<td style="width:70%">
				<%= f.check_box :d4 %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :d5 %></strong></td>
			<td style="width:70%">
				<%= f.check_box :d5 %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :d6 %></strong></td>
			<td style="width:70%">
				<%= f.check_box :d6 %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :d7 %></strong></td>
			<td style="width:70%">
				<%= f.check_box :d7 %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :interval_type %></strong></td>
			<td style="width:70%">
				<%= f.text_field :interval_type %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :interval_repeat %></strong></td>
			<td style="width:70%">
				<%= f.text_field :interval_repeat %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :minutes %></strong></td>
