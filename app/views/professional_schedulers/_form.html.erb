			<td style="width:70%">
				<%= f.text_field :date_since, {:class => "dtPicker"} %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :date_until %></strong></td>
